<div class="feed-sidebar">
  <div><strong>Subscribe</strong>:</div>
  <%= form_with model: Feed.new do |form| %>
    <%= form.label :url, 'Feed URL' %>
    <%= form.text_field :url %>
    <br>
    <%= form.submit 'Subscribe' %>
  <% end %>
  <br>
  <% if feeds.any? %>
    <div><strong>Feeds</strong>:</div>
    <% feeds.each do |feed| %>
      <%= link_to feed.title, feed %>
      <br>
    <% end %>
  <% end %>
</div>
